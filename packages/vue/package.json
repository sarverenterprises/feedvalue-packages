{
  "name": "@feedvalue/vue",
  "version": "0.1.6",
  "description": "FeedValue Vue 3 SDK - Plugin and Composables for Vue 3+",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "vue": ">=3.3.0"
  },
  "dependencies": {
    "@feedvalue/core": "workspace:^"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.18",
    "@vue/test-utils": "^2.4.0",
    "happy-dom": "^20.3.9",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18",
    "vue": "^3.5.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sarverenterprises/feedvalue-packages.git",
    "directory": "packages/vue"
  },
  "homepage": "https://feedvalue.com",
  "bugs": {
    "url": "https://github.com/sarverenterprises/feedvalue-packages/issues"
  },
  "license": "MIT",
  "author": "Sarver Enterprises",
  "keywords": [
    "feedvalue",
    "feedback",
    "widget",
    "vue",
    "vue3",
    "composable",
    "nuxt"
  ],
  "engines": {
    "node": ">=18"
  }
}
